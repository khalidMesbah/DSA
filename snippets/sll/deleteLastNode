
  void delete_last() {
    if (length <= 1) {
      delete_first();
      return;
    }
    // Get the node before tail: its order is length-1 node
    Node *previous = get_nth(length - 1);

   delete_node(tail);
    tail = previous;
    tail->next = nullptr;

    debug_verify_data_integrity();
  }
